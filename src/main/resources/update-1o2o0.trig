@prefix c:			<https://carbonldp.com/ns/v1/platform#>.
@prefix ldp:		<http://www.w3.org/ns/ldp#>.

<platform/> {
	<platform/>
		ldp:contains
			<platform/auth-tokens/>;
		ldp:member
			<platform/auth-tokens/>.
}

<platform/auth-tokens/> {
	<platform/auth-tokens/>
		a 	ldp:BasicContainer;
		c:defaultInteractionModel	ldp:Container.
}