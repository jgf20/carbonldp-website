<platform/privileges/> {
	<platform/privileges/>
		a 	ldp:BasicContainer;
		ldp:hasMemberRelation ldp:member;
		ldp:contains
			<platform/privileges/create-applications/>;
		ldp:member
			<platform/privileges/create-applications/>.
}
<platform/privileges/create-applications/> {
	<platform/privileges/create-applications/>
		a	cs:PlatformPrivilege, ldp:BasicContainer;
		c:defaultInteractionModel	ldp:RDFSource;
		ldp:hasMemberRelation ldp:member;
		cs:name	"Create Applications".
}
<platform/auth-tokens/> {
	<platform/auth-tokens/>
		a 	ldp:BasicContainer;
		c:defaultInteractionModel	ldp:Container.
}
<platform/auth-tickets/> {
	<platform/auth-tickets/>
		a 	ldp:BasicContainer;
		c:defaultInteractionModel	ldp:Container.
}