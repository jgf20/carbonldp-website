@prefix wot:		<http://xmlns.com/wot/0.1/>.
@prefix xhtml:		<http://www.w3.org/1999/xhtml#>.
@prefix xsd:		<http://www.w3.org/2001/XMLSchema#>.


<platform/> {
	<platform/>
		a 	ldp:BasicContainer;
		ldp:hasMemberRelation ldp:member;
		ldp:contains
			<platform/api/>,
			<platform/apps/>,
			<platform/agents/>,
			<platform/roles/>,
			<platform/privileges/>,
			<platform/auth-tickets/>,
			<platform/auth-tokens/>;
		ldp:member
			<platform/api/>,
			<platform/apps/>,
			<platform/agents/>,
			<platform/roles/>,
			<platform/privileges/>,
			<platform/auth-tickets/>,
			<platform/auth-tokens/>;
		cs:accessControlList <platform/~acl/>.
}
<platform/~acl/> {
	<platform/~acl/>
		a cs:AccessControlList;
		cs:accessTo <platform/>;
		cs:accessControlEntry
			<platform/~acl/#ace-235235234234>,
			<platform/~acl/#ace-235235234235>;
		cs:inheritableEntry
			<platform/~acl/#ace-235235234234>,
			<platform/~acl/#ace-235235234235>.
	<platform/~acl/#ace-235235234234>
		a cs:AccessControlEntry;
		cs:subjectClass cs:PlatformRole;
		cs:subject <platform/roles/admin/>;
		cs:granting "true"^^xsd:boolean;
		cs:permission
			cs:Read,
			cs:Update,
			cs:Delete,
			cs:CreateChild,
			cs:CreateAccessPoint,
			cs:Download,
			cs:Upload,
			cs:AddMember.
	<platform/~acl/#ace-235235234235>
		a cs:AccessControlEntry;
		cs:subjectClass cs:PlatformRole;
		cs:subject <platform/roles/app-developer/>;
		cs:granting "true"^^xsd:boolean;
		cs:permission cs:Read.
}
<platform/apps/> {
	<platform/apps/>
		a 	ldp:BasicContainer;
		ldp:hasMemberRelation ldp:member;
		cs:accessControlList <platform/apps/~acl/>.
}
<platform/apps/~acl/> {
	<platform/apps/~acl/>
		a cs:AccessControlList;
		cs:accessTo <platform/apps/>;
		cs:accessControlEntry
			<platform/apps/~acl/#ace-235235234235>.
	<platform/apps/~acl/#ace-235235234235>
		a cs:AccessControlEntry;
		cs:subjectClass cs:PlatformRole;
		cs:subject <platform/roles/app-developer/>;
		cs:granting "true"^^xsd:boolean;
		cs:permission
			cs:Read,
			cs:CreateChild.
}
<platform/agents/> {
	<platform/agents/>
		a 	ldp:BasicContainer;
		ldp:hasMemberRelation ldp:member;
		cs:accessControlList <platform/agents/~acl/>.
}
<platform/agents/~acl/> {
	<platform/agents/~acl/>
		a cs:AccessControlList;
		cs:accessTo <platform/agents/>;
		cs:accessControlEntry <platform/agents/~acl/#ace-234234235234>.
	<platform/agents/~acl/#ace-234234235234>
		a cs:AccessControlEntry;
		cs:subjectClass cs:PlatformRole;
		cs:subject <platform/roles/anonymous/>;
		cs:granting "true"^^xsd:boolean;
		cs:permission
			cs:Read,
			cs:CreateChild.
}
<platform/roles/> {
	<platform/roles/>
		a 	ldp:BasicContainer;
		ldp:hasMemberRelation ldp:member;
		ldp:contains
			<platform/roles/system/>,
			<platform/roles/admin/>,
			<platform/roles/app-developer/>,
			<platform/roles/registered-agent/>,
			<platform/roles/anonymous/>;
		ldp:member
			<platform/roles/system/>,
			<platform/roles/admin/>,
			<platform/roles/app-developer/>,
            <platform/roles/registered-agent/>,
			<platform/roles/anonymous/>.
}
<platform/roles/system/> {
	<platform/roles/system/>
		a 	cs:PlatformRole, ldp:BasicContainer;
		c:defaultInteractionModel	ldp:RDFSource;
		ldp:hasMemberRelation ldp:member;
		c:accessPoint
			<platform/roles/system/agents/>,
			<platform/roles/system/privileges/>.
}
<platform/roles/system/agents/> {
	<platform/roles/system/agents/>
		a 	ldp:DirectContainer;
		ldp:membershipResource	<platform/roles/system/>;
		ldp:hasMemberRelation	cs:agent;
		ldp:memberOfRelation	cs:platformRole.
}
<platform/roles/system/privileges/> {
	<platform/roles/system/privileges/>
		a 	ldp:DirectContainer;
		ldp:membershipResource	<platform/roles/system/>;
		ldp:hasMemberRelation	cs:platformPrivilege.
}

<platform/roles/admin/> {
	<platform/roles/admin/>
		a 	cs:PlatformRole, ldp:BasicContainer;
		c:defaultInteractionModel	ldp:RDFSource;
		ldp:hasMemberRelation ldp:member;
		c:accessPoint	
			<platform/roles/admin/agents/>,
			<platform/roles/admin/privileges/>;
		cs:platformPrivilege
			<platform/privileges/create-applications/>.
}
<platform/roles/admin/agents/> {
	<platform/roles/admin/agents/>
		a 	ldp:DirectContainer;
		ldp:membershipResource	<platform/roles/admin/>;
		ldp:hasMemberRelation	cs:agent;
		ldp:memberOfRelation	cs:platformRole.
}
<platform/roles/admin/privileges/> {
	<platform/roles/admin/privileges/>
		a 	ldp:DirectContainer;
		ldp:membershipResource	<platform/roles/admin/>;
		ldp:hasMemberRelation	cs:platformPrivilege.
}
