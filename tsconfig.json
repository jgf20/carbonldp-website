{
	"compilerOptions": {
		"module": "system",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noImplicitAny": false,
		"target": "es5",
		"moduleResolution": "node",
		"baseUrl": ".",
		"paths": {
			"app/*": [ "src/app/*" ],
			"carbonldp/*": [ "../CarbonLDP-JS-SDK/dist/*" ],
			"carbon-panel/*": [ "../carbon-panel/dist/*" ]
		}
	},
	"exclude": [
		"jspm_packages",
		"node_modules",
		"dist",
		"typings/globals",
		"typings/custom",
		"typings/index.d.ts"
	]
}