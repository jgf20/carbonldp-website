{
  "compilerOptions": {
    "module": "system",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "target": "es5",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "server/*": [ "*" ]
    },
    "lib": [
      "es2015"
    ]
  },
  "exclude": [
    "jspm_packages",
    "node_modules"
  ]
}